      <Pane visible="false" fx:id="setMousePane" layoutX="610.0" layoutY="127.0" prefHeight="237.0" prefWidth="135.0">
         <children>
            <Button fx:id="setMouseActionReaderButton" layoutX="5.0" layoutY="193.0" mnemonicParsing="false"
                    onAction="#setMouseActionReaderAction" prefHeight="25.0" prefWidth="126.0"
                    text="Start Action Read"/>
            <ChoiceBox value="BUTTON1" fx:id="setMouseActionChoiceBox" layoutX="5.0" layoutY="8.0" prefHeight="25.0"
                       prefWidth="126.0">
               <items>
                  <FXCollections fx:factory="observableArrayList">
                     <String fx:value="BUTTON1"/>
                     <String fx:value="BUTTON2"/>
                     <String fx:value="BUTTON3"/>
                  </FXCollections>
               </items>
            </ChoiceBox>
            <TextField fx:id="setMouseActionCountTextField" layoutX="6.0" layoutY="59.0" prefHeight="25.0"
                       prefWidth="126.0" promptText="count"/>
            <TextField fx:id="setMouseActionPeriodTextField" layoutX="6.0" layoutY="106.0" prefHeight="25.0"
                       prefWidth="126.0" promptText="period"/>
            <TextField fx:id="setMouseActionSleepAfterTextField" layoutX="5.0" layoutY="153.0" prefHeight="25.0"
                       prefWidth="126.0" promptText="sleepAfter"/>
            <Label layoutX="7.0" layoutY="39.0" text="Count"/>
            <Label layoutX="7.0" layoutY="86.0" text="Period"/>
            <Label layoutX="7.0" layoutY="133.0" text="Sleep After"/>
         </children>
      </Pane>
      <Label layoutX="642.0" layoutY="51.0" prefHeight="17.0" prefWidth="106.0" text="Choice Action"/>
