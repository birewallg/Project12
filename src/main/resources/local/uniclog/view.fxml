   <children>
      <ToggleButton fx:id="exit" layoutX="421.0" mnemonicParsing="false" onAction="#onExit" prefHeight="25.0"
                    prefWidth="32.0" styleClass="gui-exit-button" AnchorPane.rightAnchor="-2.0"
                    AnchorPane.topAnchor="0.0"/>
      <ToggleButton layoutX="389.0" mnemonicParsing="false" onAction="#onMin" prefHeight="24.0" prefWidth="32.0"
                    styleClass="gui-min-button" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="0.0"/>
      <Label layoutX="24.0" layoutY="4.0" text="Uniclog Software"/>
      <TextArea fx:id="textArea" layoutX="24.0" layoutY="51.0" prefHeight="263.0" prefWidth="562.0"
                styleClass="text-area"/>
      <Button layoutX="24.0" layoutY="321.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="97.0"
              styleClass="gui-button" stylesheets="@style/css.css" text="Run"/>
      <Button layoutX="489.0" layoutY="321.0" mnemonicParsing="false" onAction="#onSave" prefHeight="25.0"
              prefWidth="97.0" text="Save"/>
      <Button layoutX="489.0" layoutY="350.0" mnemonicParsing="false" onAction="#onLoad" prefHeight="25.0"
              prefWidth="97.0" text="Load"/>
      <Label layoutX="24.0" layoutY="29.0" prefHeight="17.0" prefWidth="97.0" text="Context"/>
      <ChoiceBox fx:id="setActionChoiceBox" layoutX="615.0" layoutY="75.0" prefHeight="25.0" prefWidth="126.0">
